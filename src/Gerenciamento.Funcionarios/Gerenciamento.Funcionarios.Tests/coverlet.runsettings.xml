<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>cobertura</Format>
          <Include>
			      [Gerenciamento.Funcionarios.Api]*,			
		      </Include>
          <Exclude>
            [Gerenciamento.Funcionarios.Tests]*,
            [Gerenciamento.Funcionarios.CrossCutting]*,
            [Gerenciamento.Funcionarios.Aplicacao]*,
            [Gerenciamento.Funcionarios.Dominio]*,
            [Gerenciamento.Funcionarios.Data]*,
            [*.Test]*,
            [xunit.*]*,
            [Moq]*,
            [Coverlet.*]*
          </Exclude>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>